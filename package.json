{
  "name": "koa-apis",
  "version": "1.0.0",
  "description": "apis based on koa",
  "main": "index.js",
  "scripts": {
    "start:server": "nodemon ./server --ignore ./client --ignore ./server-dist --exec babel-node nodejs ",
    "start:client": "webpack-dev-server --mode development --open --hot",
    "build:client": "find client-dist/* -not -name '.gitkeep' -print0 | xargs -0 rm -rf && webpack --mode production",
    "build:server": "find server-dist/* -not -name '.gitkeep' -print0 | xargs -0 rm -rf && babel server -d server-dist --source-maps",
    "serve:server": "node ./server-dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "~8.11.3",
    "npm": "~5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/wisnuwicak/koa-apis.git"
  },
  "keywords": [
    "koa",
    "apis"
  ],
  "proxy": "http://localhost:3000",
  "author": "wwicaksono",
  "license": "ISC",
  "homepage": "https://bitbucket.org/wisnuwicak/koa-apis#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^3.0.0",
    "dateformat": "^3.0.3",
    "dotenv": "^6.0.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-pino-logger": "^2.1.3",
    "koa-response-handler": "^2.1.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pg-promise": "^8.4.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "favicons-webpack-plugin": "0.0.9",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-manifest-plugin": "^2.0.3"
  }
}
